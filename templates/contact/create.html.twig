{% extends 'base.html.twig' %}

{% block body %}
    <h1>Create</h1>

    {{ form_start(form) }}
        {{ form_row(form.firstName) }}
        {{ form_row(form.lastName) }}
        {{ form_row(form.phoneNumber) }}
        {{ form_row(form.email) }}
        {{ form_row(form.note) }}
        <!-- Other fields -->
        <button type="submit">Submit</button>
    {{ form_end(form) }}
{% endblock %}